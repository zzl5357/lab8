</head>

<body>
  <div class="container">
    <div class="row">
      <h1>RSVP Response</h1>
    </div>
    <div class="row">
      {{#if rsvp}}
        <ul class="taskList">
          {{#each rsvp}}
            <li>{{this}}</li>
          {{/each}}
        </ul>
      {{else}}
        <p>No rsvp to show!</p>
      {{/if}}
    </div>
  </div>

  <script src="https://code.jquery.com/jquery.js"></script>
  <script src="/js/bootstrap.min.js"></script>
  <script src="/js/canvas-to-blob.min.js"></script>
  <script src="/js/tasks.js"></script>
</body>
</html>

